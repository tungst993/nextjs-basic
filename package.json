{
  "name": "mbageas-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn build",
    "prod": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext .gql --ext .graphql",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --enableOriginalQuery --depthLimit 5",
    "codegen": "graphql-codegen --config codegen/codegen.yml && yarn schema:codegen",
    "typegen": "graphql-codegen --config codegen/typegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.3.15",
